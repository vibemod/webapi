{
    "name": "vibemod/webapi",
    "type": "project",
    "require": {
        "php": ">=8.4",

        "nette/bootstrap": "^3.2.5",
        "nette/di": "^3.2.4",
        "nette/schema": "^1.3.2",
        "tracy/tracy": "^2.10.10",
        "nette/http": "^3.3.2",

        "nettrine/dbal": "^0.10.0",
        "nettrine/migrations": "^0.10.0",
        "nettrine/fixtures": "^0.9.0",
        "nettrine/orm": "^0.10.0",
        "nettrine/extra": "^0.2.0",
        "ramsey/uuid-doctrine": "^2.1.0",

        "contributte/console": "^0.10.1",
        "contributte/monolog": "^0.5.2",
        "contributte/framex": "^0.3.0",
        "contributte/tracy": "^0.7.0",
        "contributte/kernel": "^0.2.0",
        "contributte/latte": "^0.7.0",
        "contributte/event-dispatcher": "^0.9.1",
        "contributte/mcp": "^0.1.0",
        "contributte/messenger": "^0.2.0",

        "guzzlehttp/guzzle": "^7.9.3",

        "symfony/cache": "^7.4.0",
        "symfony/var-exporter": "^7.4.0"
    },
    "require-dev": {
        "contributte/qa": "^0.3.0",
        "contributte/phpunit": "^0.3.0",
        "contributte/dev": "^0.5.0",
        "contributte/phpstan": "^0.4.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src",
            "Database\\": "db"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "php-http/discovery": true
        }
    }
}
